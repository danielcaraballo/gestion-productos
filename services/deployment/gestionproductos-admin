server {
    listen 80;
    server_name productostecnologicos.otic;

    location = /favicon.ico {
        access_log off;
        log_not_found off;
    }

    location /static/ {
        alias /var/www/html/gestionproductos/services/static/;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/run/gestionproductos.sock;
    }
}
